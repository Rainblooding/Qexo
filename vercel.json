{
  "builds": [
    {
      "src": "migrate.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "templates"
      }
    },
    {
      "src": "core/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "(.*)",
      "destination": "core/wsgi.py"
    }
  ],
  "trailingSlash": true
  
  "headers": [
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, s-maxage=86400, max-age=86400"
        }
      ]
    }
  ]
}
